{
  "name": "resume",
  "version": "1.0.0",
  "description": "Resume on github",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "npm-run-all clean build:html screenshot",
    "build:html": "cross-env NODE_ENV=production webpack --progress",
    "screenshot": "npm-run-all --parallel screenshot:*",
    "screenshot:pdf": "npm run chrome --print-to-pdf=dist/resume-dillan_teagle.pdf ./dist/index.html ",
    "screenshot:png": "npm run chrome --screenshot=dist/resume-dillan_teagle.png ./dist/index.html",
    "chrome": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome  --headless --disable-gpu --page-ranges=1 --window-size=900,1200",
    "clean": "rimraf dist && mkdir dist && touch dist/.gitkeep",
    "format": "prettier --write src/**/*.{ts,tsx,css} README.md"
  },
  "author": "Dillan Teagle",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.0.6",
    "postcss-simple-vars": "^4.0.0",
    "precss": "^2.0.0",
    "prettier": "^1.17.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
